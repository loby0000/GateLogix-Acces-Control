{
  "name": "gatelogix-backend",
  "version": "2.0.0",
  "main": "src/index.js",
  "description": "Backend optimizado del sistema GateLogix - Soporta 200K usuarios mensuales",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "setup-optimization": "node src/scripts/setup-optimization.js setup",
    "health-check": "node src/scripts/setup-optimization.js health",
    "test": "echo \"Error: no test specified\" && exit 1",
    "production": "NODE_ENV=production node src/index.js",
    "cluster": "NODE_ENV=production pm2 start ecosystem.config.js"
  },
  "keywords": [
    "gatelogix",
    "access-control",
    "optimized",
    "scalable"
  ],
  "author": "GateLogix Team",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bwip-js": "^4.7.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.0",
    "morgan": "^1.10.1",
    "redis": "^5.8.2",
    "sharp": "^0.33.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "pm2": "^6.0.11"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "optimizations": {
    "maxUsers": "200000/month",
    "targetResponseTime": "<200ms",
    "cacheEnabled": true,
    "imageOptimization": true,
    "databaseIndexes": true
  }
}
